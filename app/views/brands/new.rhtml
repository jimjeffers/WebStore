<div id="status" class="panel">
  <h2>New Brand <%= link_to 'Return to brand list &gt;&gt;', brands_path, {:class => 'secondary'} %></h2>
</div>
<% form_for @brand, :html => { :multipart => true, :class => 'standard_form' } do |f| %>
<%= render :partial => 'brand_form', :locals => {:f => f} %>
<p>
  <%= f.submit 'Create' %>
  <%= link_to 'Back', brands_path %>
</p>
<% end %>